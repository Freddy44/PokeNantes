<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link href="{{ asset('bundles/productbundle/css/bootstrap.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('bundles/productbundle/css/style.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('bundles/productbundle/font/font.css') }}" rel="stylesheet" type="text/css"/>
        <script src="template/js/jquery.min.js"></script>
        <script src="template/js/bootstrap.min.js"></script>
        <script type="text/javascript">
        		$(document).ready(function(){
        			$("#demo").on("hide.bs.collapse", function(){
        				$(".li-cat");
        			});
        			$("#demo").on("show.bs.collapse", function(){
        				$(".li-cat");

        			});
        			if($("#accueil") = on.("click")){
        			$(".back").on("click", function() {
        			 $(".front").css("color", "red");
        				});
        			}
        		});
        </script>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <header>{% block header %}header{% endblock %}</header>
        <section>{% block section %}{% endblock %}</section>
        <footer>{% block footer %}footer{% endblock %}</footer>

        {% block javascripts %}{% endblock %}
    </body>
</html>
